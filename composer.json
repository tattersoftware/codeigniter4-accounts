{
	"name": "tatter/accounts",
	"description": "Multi-service account management for CodeIgniter 4",
	"keywords": [
		"codeigniter",
		"codeigniter4",
		"accounts",
		"users",
		"management"
	],
	"homepage": "https://github.com/tattersoftware/codeigniter4-accounts",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthew Gatner",
			"email": "mgatner@tattersoftware.com",
			"homepage": "https://tattersoftware.com",
			"role": "Developer"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/codeigniter4/CodeIgniter4"
		},
		{
			"type": "vcs",
			"url": "https://github.com/lonnieezell/myth-auth"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php" : "^7.2"
	},
	"require-dev": {
		"phpunit/phpunit" : "^8.5",
		"mockery/mockery": "^1.0",
		"fzaninotto/faker": "^1.9@dev",
		"codeigniter4/codeigniter4": "dev-develop",
		"myth/auth": "dev-develop",
		"stripe/stripe-php": "^7.14"
	},
	"autoload": {
		"psr-4": {
			"Tatter\\Accounts\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ModuleTests\\Support\\": "tests/_support"
		}
	},
	"scripts": {
		"test": "phpunit",
		"post-update-cmd": [
			"composer dump-autoload"
		]
	}
}
